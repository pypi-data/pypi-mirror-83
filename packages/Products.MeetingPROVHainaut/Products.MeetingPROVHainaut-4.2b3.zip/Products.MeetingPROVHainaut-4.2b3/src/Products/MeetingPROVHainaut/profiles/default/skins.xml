<?xml version="1.0"?>

<object name="portal_skins"
        meta_type="Plone Skins Tool"
        allow_any="False"
        cookie_persistence="False"
        request_varname="plone_skin">

 <object name="meetingprovhainaut_images"
         meta_type="Filesystem Directory View"
         directory="MeetingPROVHainaut/skins/meetingprovhainaut_images"/>
 <object name="meetingprovhainaut_styles"
         meta_type="Filesystem Directory View"
         directory="MeetingPROVHainaut/skins/meetingprovhainaut_styles"/>
 <object name="meetingprovhainaut_templates"
         meta_type="Filesystem Directory View"
         directory="MeetingPROVHainaut/skins/meetingprovhainaut_templates"/>
 
 <skin-path name="*">
  <layer insert-after="custom"
         name="meetingprovhainaut_images"/>
  <layer insert-after="custom"
         name="meetingprovhainaut_styles"/>
  <layer insert-after="custom"
         name="meetingprovhainaut_templates"/>
 </skin-path>

</object>
