<?xml version="1.0"?>
<object name="portal_less">

 <stylesheet title="cpskin.diazotheme.vicinity LESS Variables"
    id="++theme++cpskin.diazotheme.vicinity/less/variables.less"
    media=""
    enabled="1"
    rendering="link"
    compression="none"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.vicinity'"
    applyPrefix="True"
    insert-before="portal_resources/cpskin/variables.less" />

 <stylesheet title="cpskin.diazotheme.vicinity LESS Styles"
    id="++theme++cpskin.diazotheme.vicinity/less/styles.less"
    media="all"
    enabled="1"
    rendering="link"
    compression="none"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.vicinity'"
    applyPrefix="True"
    insert-before="portal_resources/cpskin/styles.less" />
 
 <stylesheet title="cpskin.diazotheme.vicinity LESS Mobile Styles"
    id="++theme++cpskin.diazotheme.vicinity/less/mobile.less"
    media="handheld, screen and (max-device-width: 600px), screen and (max-width: 600px)"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.vicinity'"
    compression="none"
    insert-after="++theme++cpskin.diazotheme.vicinity/less/styles.less" />

 <stylesheet title="cpskin.diazotheme.vicinity LESS Large Mobile Styles"
    id="++theme++cpskin.diazotheme.vicinity/less/mobilelarge.less"
    media="handheld, screen and (min-device-width: 768px) and (max-device-width: 1024px), screen and (min-device-width: 768px) and (max-width: 1024px)"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.vicinity'"
    compression="none"
    insert-after="++theme++cpskin.diazotheme.vicinity/less/mobile.less" />

</object>
