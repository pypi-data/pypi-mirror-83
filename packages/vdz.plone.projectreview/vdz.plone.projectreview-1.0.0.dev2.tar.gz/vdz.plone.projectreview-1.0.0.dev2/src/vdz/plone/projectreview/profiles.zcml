<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:genericsetup="http://namespaces.zope.org/genericsetup"
    i18n_domain="plone">

  <genericsetup:registerProfile
      name="default"
      title="vdz.plone.projectreview"
      directory="profiles/default"
      description="VDZ-Verwertungsmanagement"
      provides="Products.GenericSetup.interfaces.EXTENSION"
      />

  <genericsetup:registerProfile
      name="uninstall"
      title="Remove vdz.plone.projectreview"
      directory="profiles/uninstall"
      description="Entfernt das VDZ-Verwertungsmanagement aus dieser Plone-Installation"
      provides="Products.GenericSetup.interfaces.EXTENSION"
      />

  <!-- [ Migrationsschritte (Upgrade steps) ... [
