<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/@@ploneform-macros/form"
      i18n:domain="plone">
<body>
            <metal:title-slot fill-slot="title" />
            <metal:define use-macro="titlelessform">
                <metal:fields-slot fill-slot="fields" />
                <metal:description-slot fill-slot="description" />
                <metal:actions-slot fill-slot="actions" />
            </metal:define>
</body>
</html>
