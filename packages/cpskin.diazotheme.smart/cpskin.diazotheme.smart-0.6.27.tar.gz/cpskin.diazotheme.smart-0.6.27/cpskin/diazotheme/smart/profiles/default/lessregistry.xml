<?xml version="1.0"?>
<object name="portal_less">

 <stylesheet title="cpskin.diazotheme.smart LESS Variables"
    id="++theme++cpskin.diazotheme.smart/less/variables.less"
    media=""
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.smart'"
    compression="none"
    insert-before="portal_resources/cpskin/variables.less" />

 <stylesheet title="cpskin.diazotheme.smart LESS Styles"
    id="++theme++cpskin.diazotheme.smart/less/styles.less"
    media="all"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.smart'"
    compression="none"
    insert-before="portal_resources/cpskin/styles.less" />
    
<stylesheet title="cpskin.diazotheme.smart LESS Namur"
    id="++theme++cpskin.diazotheme.smart/less/namur.less"
    media="all"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.smart'"
    compression="none"
    insert-after="++theme++cpskin.diazotheme.smart/less/styles.less" />

<stylesheet title="cpskin.diazotheme.smart LESS Mobile Styles"
    id="++theme++cpskin.diazotheme.smart/less/mobile.less"
    media="handheld, screen and (max-device-width: 600px), screen and (max-width: 600px)"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.smart'"
    compression="none"
    insert-after="++theme++cpskin.diazotheme.smart/less/namur.less" />

 <stylesheet title="cpskin.diazotheme.smart LESS Large Mobile Styles"
    id="++theme++cpskin.diazotheme.smart/less/mobilelarge.less"
    media="handheld, screen and (min-device-width: 768px) and (max-device-width: 1024px), screen and (min-device-width: 768px) and (max-width: 1024px)"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.smart'"
    compression="none"
    insert-after="++theme++cpskin.diazotheme.smart/less/mobile.less" />
 
  <stylesheet title="cpskin.diazotheme.smart LESS Print"
    id="++theme++cpskin.diazotheme.smart/less/printnamur.less"
    media="print"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.smart'"
    compression="none"
    insert-after="++theme++cpskin.diazotheme.smart/cpskin/mobilelarge.less" />


</object>
