<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="imio.gdpr"
    purge="False">

    <configlet
        title="GDPR"
        action_id="gdpr"
        appId="imio.gdpr"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@gdpr-settings"
        icon_expr="string:$portal_url/++resource++imio.gdpr/gdpr.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
