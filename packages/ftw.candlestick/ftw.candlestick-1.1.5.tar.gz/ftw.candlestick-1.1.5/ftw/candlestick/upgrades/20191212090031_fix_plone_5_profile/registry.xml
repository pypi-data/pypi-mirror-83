<registry>

    <records prefix="plone.resources/ftw_candlestick"
             interface="Products.CMFPlone.interfaces.IResourceRegistry"
             remove="True" />

    <records prefix="plone.resources/ftw_candlestick_integration"
             interface="Products.CMFPlone.interfaces.IResourceRegistry"
             remove="True" />

    <records prefix="plone.bundles/ftw-candlestick-resources"
             interface="Products.CMFPlone.interfaces.IBundleRegistry">
        <value key="resources" purge="True"></value>
        <value key="enabled">True</value>
        <value key="depends">plone</value>
        <value key="stub_js_modules">jquery</value>
        <value key="compile">False</value>
        <value key="jscompilation">++resource++ftw.candlestick/candlestick-compiled.js</value>
        <value key="last_compilation">2019-12-11 10:00:00</value>
        <value key="merge_with">default</value>
    </records>

</registry>
