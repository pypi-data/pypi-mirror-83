<html xmlns="http://www.w3.org/1999/xhtml"
	    xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
	    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="cpskin.localfood" lang="en">

<div class="viewlet-labels documentByLine"
     tal:condition="view/can_view">
  <tal:while repeat="label context/labels">
  <span class="label" tal:content="label"></span><tal:if condition="not: repeat/label/end">, </tal:if>
  </tal:while>
</div>

</html>
